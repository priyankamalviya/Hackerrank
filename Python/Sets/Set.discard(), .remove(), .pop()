n = int(input())
s = set(map(int, input().split())) 
N=int(input())
for i in range(N) :
    option=input().split()
    if option[0]=="pop" :
        s.pop()
    elif option[0]=="remove" :
        s.remove(int(option[1]))
    elif option[0]=="discard" :
        s.discard(int(option[1]))
print (sum(s))
